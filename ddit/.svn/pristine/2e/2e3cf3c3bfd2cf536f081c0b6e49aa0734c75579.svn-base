package kr.or.ddit.dto;

public class ExampleVO {
	private String example_no;  //문항번호
	private String example_content;  //문항내용
	private String exam_no;  //시험문제번호
	private int example_number; //시험문제문항번호
	
	public String getExample_no() {
		return example_no;
	}
	public void setExample_no(String example_no) {
		this.example_no = example_no;
	}
	public String getExample_content() {
		return example_content;
	}
	public void setExample_content(String example_content) {
		this.example_content = example_content;
	}
	public String getExam_no() {
		return exam_no;
	}
	public void setExam_no(String exam_no) {
		this.exam_no = exam_no;
	}
	public int getExample_number() {
		return example_number;
	}
	public void setExample_number(int example_number) {
		this.example_number = example_number;
	}
	@Override
	public String toString() {
		return "ExampleVO [example_no=" + example_no + ", example_content=" + example_content + ", exam_no=" + exam_no
				+ ", example_number=" + example_number + "]";
	}


	
	
}
