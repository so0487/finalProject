package kr.or.ddit.dto;

import java.sql.Date;

public class PostVO {
	private String post_no; // 게시글번호
	private Date post_update_date; // 수정날짜
	private String post_content; // 내용
	private String post_status; // 상태
	private String board_no; // 게시판번호
	private int post_viewcnt; // 조회수
	private Date post_reg_date; // 작성날짜
	private String post_writer; // 회원번호
	private String post_title; // 제목

	public String getPost_no() {
		return post_no;
	}

	public void setPost_no(String post_no) {
		this.post_no = post_no;
	}

	public Date getPost_update_date() {
		return post_update_date;
	}

	public void setPost_update_date(Date post_update_date) {
		this.post_update_date = post_update_date;
	}

	public String getPost_content() {
		return post_content;
	}

	public void setPost_content(String post_content) {
		this.post_content = post_content;
	}

	public String getPost_status() {
		return post_status;
	}

	public void setPost_status(String post_status) {
		this.post_status = post_status;
	}

	public String getBoard_no() {
		return board_no;
	}

	public void setBoard_no(String board_no) {
		this.board_no = board_no;
	}

	public Date getPost_reg_date() {
		return post_reg_date;
	}

	public void setPost_reg_date(Date post_reg_date) {
		this.post_reg_date = post_reg_date;
	}

	public String getPost_writer() {
		return post_writer;
	}

	public void setPost_writer(String post_writer) {
		this.post_writer = post_writer;
	}

	public String getPost_title() {
		return post_title;
	}

	public void setPost_title(String post_title) {
		this.post_title = post_title;
	}

	public int getPost_viewcnt() {
		return post_viewcnt;
	}

	public void setPost_viewcnt(int post_viewcnt) {
		this.post_viewcnt = post_viewcnt;
	}

	@Override
	public String toString() {
		return "PostVO [post_no=" + post_no + ", post_update_date=" + post_update_date + ", post_content="
				+ post_content + ", post_status=" + post_status + ", board_no=" + board_no + ", post_viewcnt="
				+ post_viewcnt + ", post_reg_date=" + post_reg_date + ", post_writer=" + post_writer + ", post_title="
				+ post_title + "]";
	}

}
