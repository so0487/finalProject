package kr.or.ddit.dto;

public class ScheduleCategoryVO {
	private String schedule_category_name;  //일정카테고리이름
	private String schedule_category_no;  //일정카테고리번호
	private String schedule_category_role;  //일정권한
	
	
	public String getSchedule_category_name() {
		return schedule_category_name;
	}
	public void setSchedule_category_name(String schedule_category_name) {
		this.schedule_category_name = schedule_category_name;
	}
	public String getSchedule_category_no() {
		return schedule_category_no;
	}
	public void setSchedule_category_no(String schedule_category_no) {
		this.schedule_category_no = schedule_category_no;
	}
	public String getSchedule_category_role() {
		return schedule_category_role;
	}
	public void setSchedule_category_role(String schedule_category_role) {
		this.schedule_category_role = schedule_category_role;
	}
	@Override
	public String toString() {
		return "ScheduleCategoryVO [schedule_category_name=" + schedule_category_name + ", schedule_category_no="
				+ schedule_category_no + ", schedule_category_role=" + schedule_category_role + "]";
	}
	
	
	
}
