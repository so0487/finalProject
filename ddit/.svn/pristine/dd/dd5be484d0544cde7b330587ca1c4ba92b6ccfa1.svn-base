<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ page trimDirectiveWhitespaces="true"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %>

<c:set var="cri" value="${pageMaker.cri }" />



	<div class="content-body">
		
       <!-- Page Headings Start -->
        <div class="row justify-content-between align-items-center mb-10">

            <!-- Page Heading Start -->
            <div class="col-12 col-lg-auto mb-20">
                <div class="page-heading">
                    <h3 class="title">시험 응시 수강생 목록</h3>
                </div>
                
            </div><!-- Page Heading End -->

        </div><!-- Page Headings End -->
		
		<div class="row">
			<div style="margin-left:300px;">
				<a href="lectureTestList?lecture_no=${testvo.lecture_no }"><button class="btn outline" style="border:1px solid blue">뒤로</button></a>
			</div>
		</div><br>
		<div class="row">
		
			<div class="col-lg-3 mb-20">
	             <div class="box-body">
	                 <div class="tab-content">	                     
                         <table class="table" >
                               <thead>
                                   <tr>
                                      <th>번호</th>
                                      <th>이름</th>
                                      <th>점수</th>
                                      <th>반영비율</th>
                                      <th>시험지</th>
                                   </tr>
                               </thead>
                               <tbody>
								 <c:forEach items="${testHisList }" var="test" varStatus="status">
                                   <tr>
                                       <td><a href="#" onClick=markTestBtn("${test.TEST_HISTORY_NO }");return false;>${status.count } </a></td>
                                       <td><a href="#" onClick=markTestBtn("${test.TEST_HISTORY_NO }");return false;>${test.MEMBER_NAME } </a></td>
                                       <td>${test.TEST_HISTORY_SCORE } / ${testvo.test_score}</td>
                                       <td>${testvo.test_ratio }</td>
                                       <td><button class="btn btn-info" style="font-size:12px;" onClick=markTestBtn("${test.TEST_HISTORY_NO }");return false;>채점하기</button></td>
                                   </tr>
                              	 </c:forEach>
                               </tbody>
                           </table>
	                 </div>
	             </div>
		</div>		
	</div>
</div>
		


	
<script>
	function markTestBtn(ele){
		location.href="markStudentTest?test_history_no="+ele;
	}
	

</script>
<style>


</style>

