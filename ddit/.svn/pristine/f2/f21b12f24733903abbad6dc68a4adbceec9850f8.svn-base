package kr.or.ddit.dto;

public class ReviewVO {
	private String review_contetn;  //한줄평
	private String student_id;  //수강생id
	private int review_star;  //별점
	private String review_no;  //수강후기번호
	private String class_no;  //회차id
	
	
	public String getReview_contetn() {
		return review_contetn;
	}
	public void setReview_contetn(String review_contetn) {
		this.review_contetn = review_contetn;
	}
	public String getStudent_id() {
		return student_id;
	}
	public void setStudent_id(String student_id) {
		this.student_id = student_id;
	}
	public int getReview_star() {
		return review_star;
	}
	public void setReview_star(int review_star) {
		this.review_star = review_star;
	}
	public String getReview_no() {
		return review_no;
	}
	public void setReview_no(String review_no) {
		this.review_no = review_no;
	}
	public String getClass_no() {
		return class_no;
	}
	public void setClass_no(String class_no) {
		this.class_no = class_no;
	}
	@Override
	public String toString() {
		return "ReviewVO [review_contetn=" + review_contetn + ", student_id=" + student_id + ", review_star="
				+ review_star + ", review_no=" + review_no + ", class_no=" + class_no + "]";
	}
	
}
